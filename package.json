{
  "name": "geospatial-api-nodejs",
  "scripts": {
    "dev": "concurrently --kill-others \"yarn --cwd backend dev\" \"yarn --cwd frontend start\"",
    "start": "concurrently --kill-others \"yarn --cwd backend start\" \"yarn --cwd frontend start\"",
    "test": "yarn --cwd backend test && yarn --cwd frontend test",
    "postinstall": "yarn --cwd backend install && yarn --cwd frontend install"
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "private": true
}
